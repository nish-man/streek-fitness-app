{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "buildCommand": "expo export --platform web",
        "outputDirectory": "dist",
        "env": {
          "EXPO_PUBLIC_APP_NAME": "Streek Fitness App",
          "EXPO_PUBLIC_APP_VERSION": "1.0.0"
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "rewrites": [
    {
      "source": "/main.bundle.js",
      "destination": "/static/js/main.bundle.js"
    }
  ]
}
